<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="rootElement" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">

    <children>

      <TitledPane animated="true" text="Import options">
        <content>
          <VBox minHeight="0.0" minWidth="0.0" >
             <children>
                 <Label text="Import file with table data" />
                 <Button fx:id="selectFile" onAction="#loadDataFromFile" text="Import">
                     <VBox.margin>
                         <Insets bottom="10.0" />
                     </VBox.margin>
                 </Button>
   
                 <Label text="Import data from lectures example" />
                 <Button fx:id="importDataFromLecturesExample" onAction="#loadDataFromLecturesExample" text="Import">
                     <VBox.margin>
                         <Insets bottom="10.0" />
                     </VBox.margin>
                 </Button>
   
                 <Label text="Import default data" />
                 <Button fx:id="importDefaultData" onAction="#loadDefaultData" text="Import">
                     <VBox.margin>
                         <Insets bottom="10.0" />
                     </VBox.margin>
                 </Button>
             </children>
          </VBox>
        </content>
      </TitledPane>
    </children>

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</fx:root>
